    "@tanstack/react-query": "^4.29.12",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.27.0",
    "iconv-lite": "^0.6.3",
    "mime-types": "^2.1.35",
    "next": "13.4.4",
    "pdfjs-dist": "^3.7.107",
    "postgres": "^3.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.34.3",
    "@swc-node/register": "^1.6.5",
    "@tailwindcss/forms": "^0.5.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "20.2.5",
