    "@tanstack/react-query": "^4.33.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.5",
    "iconv-lite": "^0.6.3",
    "next": "13.4.19",
    "pdfjs-dist": "^3.9.179",
    "postgres": "^3.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@swc-node/register": "^1.6.7",
    "@tailwindcss/forms": "^0.5.5",
    "@types/node": "20.5.6",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "^10.4.15",
    "drizzle-kit": "^0.19.13",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2.2",
    "vitest": "^0.34.3"
