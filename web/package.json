{
  "name": "web",
  "version": "0.1.0",
  "scripts": {
    "setup": "sh ./scripts/setup.sh",
    "swc": "node -r @swc-node/register",
    "reset": "sh ./scripts/reset.sh",
    "dev": "next dev",
    "test": "vitest",
    "test-e2e": "sh ./tests/test-e2e.sh",
    "pre-commit": "pnpm exec lint-staged && CI=true pnpm test && tsc --noEmit",
    "export-vat": "pnpm run swc ./scripts/vat/exportVATReport.ts",
    "export-sie": "pnpm run swc ./scripts/annualReport/exportSie.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.2",
    "iconv-lite": "^0.6.3",
    "next": "14.0.4",
    "pdfjs-dist": "4.0.269",
    "postgres": "^3.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@swc-node/register": "^1.6.8",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.20.8",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.4.0",
    "typescript": "5.3.3",
    "vitest": "^1.1.0"
  }
}
