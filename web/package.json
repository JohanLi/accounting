    "@types/react-dom": "18.2.4",
    "autoprefixer": "^10.4.14",
    "drizzle-kit": "^0.19.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.4",
    "vitest": "^0.31.1"
  }
